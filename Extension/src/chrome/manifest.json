{
    "manifest_version": 2,
    "name": "Psychic giggle",
    "version": "1.0.0",
    "description": "Use psychic powers to sync videos and chat with friends",
    "icons": {
      "16": "img/16.png",
      "48": "img/48.png",
      "128": "img/128.png"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["js/content.js"]
        }
    ],
    "permissions": []
  }